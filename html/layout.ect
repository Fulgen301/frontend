<!doctype html>
<html>
  <head>
    <title>Clonkspot</title>
    <meta charset='utf-8'>
    <link rel="stylesheet" href="/css/clonkspot.css" type="text/css" charset="utf-8">
  </head>
  <body>
    <header>
      <a href="/">
        <img src="/images/header.jpg" alt="">
        <div class="logo">
          <img src="/images/logo.png" alt="clonkspot - your hub for Clonk.">
        </div>
      </a>


      <nav>
        <a href="/">
          <img src="/images/home.png" alt="Home">
        </a>
        <% for key, item of @t.nav : %>
        <a href="<%- @data.nav[key] %>">
          <b><%= item[0] %></b>
          <small><%= item[1] %></small>
        </a>
        <% end %>
      </nav>
    </header>
    <% content %>
    <footer>
    </footer>
  </body>
</html>
