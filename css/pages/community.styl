/* Community */

@import "nib"

#quota {
  absolute: top 189px left 720px
  background-color: #f4f4f4
}

#projects {
  width:305px
}

#projects2 {
  width:305px
  absolute: bottom 390px left 370px
}

ul {
  list-style: none
  padding 0px
  margin 0px
  width: 100px
}

li {
  
  padding-left: 0px
  display: inline
}

#externalLinks{
  
  a:link, a:visited {
    display: block
    font-weight: bold
    color: black
    background-color: rgba(228, 228, 228, 0.5)
    width: 500px
    text-align: center
    padding: 14px
    text-decoration:none
    margin-bottom: -2px
  }  
  a:hover, a:active {
  transition:.2s;
  background-color: #434343
  }
}

